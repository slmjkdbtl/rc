[core]
	pager = less -+FX
	ignorecase = false
	excludesfile = /Users/tga/.gitignore
[user]
	email = tga@space55.xyz
	name = tga
[alias]
	conflicts = diff --name-only --diff-filter=U
	root = rev-parse --show-toplevel
	file = log --follow -p --
	slog = log --oneline
	flog = log --pretty=fuller
	logf = log -p
	commitf = commit --amend --no-edit
	co = checkout
	prev = checkout HEAD~1
	conflicts = diff --name-only --diff-filter=U
	next = ! git log --reverse --pretty=%H master | grep -A 1 $(git rev-parse HEAD) | tail -n1 | xargs git checkout
[credential]
	helper = store
[pull]
	rebase = false
[pager]
	branch = false
[web]
    browser = open
[browser "chrome"]
    cmd = open -a 'Google Chrome.app'
