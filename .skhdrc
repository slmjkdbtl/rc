cmd + shift - 3 : screencapture -dx $HOME/Desktop/$(date +%s).png
cmd + shift - 4 : screencapture -idx $HOME/Desktop/$(date +%s).png
cmd + shift - 5 : screencapture -idUx $HOME/Desktop/$(date +%s).png
cmd + shift - 6 : shortcuts run "Screenshot OCR"
cmd + shift - o : osascript -e 'tell application "Finder" to if "Finder" is in (name of first application process whose frontmost is true) then do shell script "open -a WezTerm && wezterm cli spawn --cwd " & (quoted form of POSIX path of (folder of the front window as string) as string)'
