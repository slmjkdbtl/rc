# wengwengweng

cmd + shift - 2 : screencapture -wd $HOME/Desktop/$(date +%s).png
cmd + shift - 3 : screencapture -d $HOME/Desktop/$(date +%s).png
cmd + shift - 4 : screencapture -id $HOME/Desktop/$(date +%s).png
cmd + shift - 5 : screencapture -idU $HOME/Desktop/$(date +%s).png
cmd + shift - 6 : /usr/local/bin/ocr | pbcopy
cmd + shift - p : open -a 'Digital Color Meter.app'
cmd + shift - o : osascript -e 'tell application "Finder" to if "Finder" is in (name of first application process whose frontmost is true) then do shell script "open -a iTerm " & (quoted form of POSIX path of (folder of the front window as string) as string)'
